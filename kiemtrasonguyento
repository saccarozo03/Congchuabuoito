#include<stdio.h>
#include<math.h>
int songuyento(long long n){int cnt=0;
    for(int i=2;i<sqrt(n);i++){
        if(n%i==0){
            return 0;
        }
    }
    return 1;
    
}

int main(){
    long long n;
    scanf("%lld",&n);
    int a=songuyento(n);
    if(a){
        printf("YES");
    }else
    printf("NO");
}
